{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ES2022",
    "esModuleInterop": true,
    "strict": true,
    "noImplicitOverride": true,
    "module": "NodeNext",
    "sourceMap": true,
    "declaration": true,
    "lib": ["es2022", "dom", "DOM.Iterable"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src"],
}
